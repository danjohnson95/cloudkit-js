{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "examples",
    "secrets.ts"
  ],
}
